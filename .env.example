# FastAPI Listener
PORT=8000

# Twilio WhatsApp integration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=whatsapp:+14155238886

## Supabase
DATABASE_URL=database_connection_string_here

# OpenAI API for agents
OPENAI_API_KEY=your_openai_api_key_here

# === LANGUAGE AGENTS CONFIGURATION ===
# Model for text-based conversations (English, German, Spanish)
LANGUAGE_AGENT_MODEL=gpt-4o
LANGUAGE_AGENT_TEMPERATURE=0.3
LANGUAGE_AGENT_MAX_TOKENS=1000

# === IMAGE AGENT CONFIGURATION ===
# Model for image analysis and processing
IMAGE_AGENT_MODEL=gpt-4o
IMAGE_AGENT_TEMPERATURE=0.1
IMAGE_AGENT_MAX_TOKENS=500

# === MANAGER AGENT CONFIGURATION ===
# Model for routing and coordination
MANAGER_AGENT_MODEL=gpt-4o-mini
MANAGER_AGENT_TEMPERATURE=0.2
MANAGER_AGENT_MAX_TOKENS=300

# OpenAI Vector Store IDs for different languages
VECTOR_STORE_EN=your_english_vector_store_id_here
VECTOR_STORE_DE=your_german_vector_store_id_here
VECTOR_STORE_ES=your_spanish_vector_store_id_here

# ElevenLabs API for audio transcription
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# App configuration
ENVIRONMENT=development
DEBUG=true

# Test phone numbers (comma-separated)
TEST_PHONE_NUMBERS=whatsapp:+1234567890,whatsapp:+0987654321
