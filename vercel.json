{
  "version": 2,
  "builds": [
    {
      "src": "app/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12.6",
        "installCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/app.py"
    }
  ]
}
